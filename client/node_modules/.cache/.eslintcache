form-control\"\n                  value={address.city}\n                  onChange={handleChange}\n                  required\n                />\n              </div>\n            </Col>\n            <Col xs=\"12\" lg=\"3\">\n              <div className=\"form-group\">\n                <label>State</label>\n                <input\n                  type={\"text\"}\n                  name={\"state\"}\n                  className=\"form-control\"\n                  value={address.state}\n                  onChange={handleChange}\n                  required\n                />\n              </div>\n            </Col>\n            <Col xs=\"12\" lg=\"3\">\n              <div className=\"form-group\">\n                <label>Country</label>\n                <input\n                  type={\"text\"}\n                  name={\"country\"}\n                  className=\"form-control\"\n                  value={address.country}\n                  onChange={handleChange}\n                  required\n                />\n              </div>\n            </Col>\n            <Col xs=\"12\" lg=\"3\">\n              <div className=\"form-group\">\n                <label>PinCode</label>\n                <input\n                  type=\"number\"\n                  name={\"pinCode\"}\n                  className=\"form-control\"\n                  value={address.pinCode}\n                  onChange={handleChange}\n                  required\n                />\n              </div>\n            </Col>\n          </Row>\n          <div className=\"add-address-actions\">\n            <Button onClick={saveAddressToDb} text=\"Add Address\" />\n          </div>\n        </form>\n      </div>\n    </>\n  );\n\n  return (\n    <div className=\"contact\">\n      <Row>\n        <Col xs=\"12\" md=\"8\">\n          <h4>Delivery Address</h4>\n          {showAddress()}\n          <hr />\n        </Col>\n        <Col xs=\"12\" md=\"4\">\n          <div className=\"cart\">\n            <div class