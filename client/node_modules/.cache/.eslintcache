size: 12, order: 1 }}\n            sm={{ size: 12, order: 1 }}\n            md={{ size: 3, order: 1 }}\n            lg={{ size: 3, order: 1 }}\n          >\n            <div className=\"brand\">\n              <Link to=\"/\" className=\"logo\">\n                <Logo />\n              </Link>\n            </div>\n          </Col>\n          <Col\n            xs={{ size: 12, order: 2 }}\n            sm={{ size: 12, order: 2 }}\n            md={{ size: 4, order: 1 }}\n            lg={{ size: 5, order: 3 }}\n            className=\"desktop-hidden\"\n          >\n            <div className=\"header-links\">\n              <Button ariaLabel=\"open the menu\" icon={<BarsIcon />} />\n              <Link to=\"/\" className=\"logo\">\n                <Logo />\n              </Link>\n              <Search />\n              <CartIcon cartItems={cart.length} onClick={handleAddToCart} />\n            </div>\n          </Col>\n          <Col\n            xs={{ size: 12, order: 2 }}\n            sm={{ size: 12, order: 2 }}\n            md={{ size: 9, order: 1 }}\n            lg={{ size: 9, order: 3 }}\n          >\n            <Navbar color=\"light\" light expand=\"md\" className=\"mt-1 mt-md-0\">\n              <CartIcon\n                className=\"d-none d-md-block\"\n                cartItems={cart.length}\n                onClick={handleAddToCart}\n              />\n              <span className=\"search float-right p-1 ml-1 mr-1\">\n                <Search />\n              </span>\n              <Nav navbar>\n                <NavItem>\n                  <NavLink tag={ActiveLink} to=\"/shop\" activeClassName=\"active\">\n                    Shop\n                  </NavLink>\n                </NavItem>\n                {user ? (\n                  <UncontrolledDropdown nav inNavbar>\n                    <DropdownToggle nav>\n                      {user ? user.email.split(\"@\")[0] : \"Welcome\"}\n                      <span className=\"fa fa-chevron-down dropdown-caret\"></span>\n                    </DropdownToggle>\n                    {user.role == \"admin\" ? (\n                      <DropdownMenu right>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/history\")}\n                        >\n                          Dashboard\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/product\")}\n                        >\n                          Product\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/products\")}\n                        >\n                          Products\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/category\")}\n                        >\n                          category\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/sub\")}\n                        >\n                          sub\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/coupon\")}\n                        >\n                          coupon\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/password\")}\n                        >\n                          Settings\n                        </DropdownItem>\n                        <DropdownItem onClick={logout}>Sign Out</DropdownItem>\n                      </DropdownMenu>\n                    ) : (\n                      <DropdownMenu right>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/history\")}\n                        >\n                          Dashboard\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/wishlist\")}\n                        >\n                          Wishlist\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/password\")}\n                        >\n                          Settings\n                        </DropdownItem>\n                        <DropdownItem onClick={logout}>Sign Out</DropdownItem>\n                      </DropdownMenu>\n                    )}\n                  </UncontrolledDropdown>\n                ) : (\n                  <UncontrolledDropdown nav inNavbar>\n                    <DropdownToggle nav>\n                      Welcome!\n                      <span className=\"fa fa-chevron-down dropdown-caret\"></span>\n                    </DropdownToggle>\n                    <DropdownMenu right>\n                      <DropdownItem onClick={() => history.push(\"/login\")}>\n                        Login\n                      </DropdownItem>\n                      <DropdownItem onClick={() => history.push(\"/register\")}>\n                        Sign Up\n                      </DropdownItem>\n                    </DropdownMenu>\n                  </UncontrolledDropdown>\n                )}\n              </Nav>\n            </Navbar>\n          </Col>\n        </Row>\n      </Container>\n\n      {/* hidden cart drawer */}\n      {/* <div\n        className={isCartOpen ? \"mini-cart-open\" : \"hidden-mini-cart\"}\n        aria-hidden={`${isCartOpen ? false : true}`}\n      >\n        <div className=\"mini-cart\">\n          <SideDrawer />\n          Cart\n        </div>\n        <div\n          className={\n            isCartOpen ? \"drawer-backdrop dark-overflow\" : \"drawer-backdrop\"\n          }\n          onClick={toggleCart}\n        />\n      </div> */}\n\n  