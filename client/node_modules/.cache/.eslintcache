data);\n\n      //load related\n      getRelated(res.data._id).then((res) => setRelated(res.data));\n    });\n\n  const onStarClick = (newRating, name) => {\n    setStar(newRating);\n    // console.table(newRating, name);\n    productStar(name, newRating, user.token).then((res) => {\n      console.log(\"Rating clicked\", res.data);\n      loadSingleProduct();\n    });\n  };\n\n  return (\n    <div className=\"container-fluid\">\n      <div className=\"row pt-4\">\n        <SingleProduct\n          product={product}\n          onStarClick={onStarClick}\n          star={star}\n        />\n      </div>\n\n      <div className=\"row\">\n        <div className=\"col text-center pt-5 pb-5\">\n          <hr />\n          <h4>Related Products</h4>\n          <hr />\n\n          <div className=\"row pb-5\">\n            {related.length ? (\n              related.map((r) => (\n                <div key={r._id} className=\"col-md-4\">\n                  <ProductCard product={r} />\n                </div>\n              ))\n            ) : (\n              <div className=\"text-center col\">No products found</div>\n            )}\n            {/* {JSON.stringify(related)} */}\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Product;\n","/Users/apple/Downloads/MOON/client/src/pages/Cart.js",["315","316"],"/Users/apple/Downloads/MOON/client/src/pages/auth/RegisterComplete.js",[],"/Users/apple/Downloads/MOON/client/src/pages/auth/Login.js",[],"/Users/apple/Downloads/MOON/client/src/pages/admin/AdminDashboard.js",[],"/Users/apple/Downloads/MOON/client/src/pages/user/History.js",[],"/Users/apple/Downloads/MOON/client/src/pages/auth/ForgotPassword.js",[],"/Users/apple/Downloads/MOON/client/src/pages/user/Wishlist.js",[],"/Users/apple/Downloads/MOON/client/src/pages/user/Password.js",[],"/Users/apple/Downloads/MOON/client/src/pages/admin/category/CategoryCreate.js",["317"],"import React, { useState, useEffect } from \"react\";\nimport { toast } fr