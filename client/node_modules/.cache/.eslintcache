/client/src/components/modal/RatingModal.js",["350"],"import React, { useState } from \"react\";\nimport { Modal } from \"antd\";\nimport { toast } from \"react-toastify\";\nimport { useSelector } from \"react-redux\";\nimport { StarOutlined } from \"@ant-design/icons\";\nimport { useHistory, useParams } from \"react-router-dom\";\nimport Button from \"../Button\";\nimport { ReviewIcon } from \"../Icon\";\n\nconst RatingModal = ({ children }) => {\n  const { user } = useSelector((state) => ({ ...state }));\n  const [modalVisible, setModalVisible] = useState(false);\n\n  let history = useHistory();\n  let { slug } = useParams();\n\n  const handleModal = () => {\n    if (user && user.token) {\n      setModalVisible(true);\n    } else {\n      history.push({\n        pathname: \"/login\",\n        state: { from: `/product/${slug}` },\n      });\n    }\n  };\n\n  return (\n    <>\n      <div onClick={handleModal}>\n        <Button\n          variant=\"primary\"\n          text={user ? \"Leave Rating\" : \"Login To Review\"}\n          icon={<ReviewIcon />}\n        />\n      </div>\n      <Modal\n        title=\"Leave Your Rating\"\n        centered\n        visible={modalVisible}\n        onOk={() => {\n          setModalVisible(false);\n          toast.success(\"Thanks for your review. It will appear soon\");\n        }}\n        onCancel={() => setModalVisible(false)}\n      >\n        {children}\n      </Modal>\n    </>\n  );\n};\n\nexport default RatingModal;\n","/Users/apple/Downloads/MOON/client/src/components/forms/Search.js",[],"/Users/apple/Downloads/MOON/client/src/components/Button/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/Icon/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/CartIcon/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/Carousel/CarouselContainer.js",[],"/Users/apple/Downloads/MOON/client/src/components/nav/Logo.js",[],"/Users/apple/Downloads/MOON/client/src/components/Input/index.js",["351","352"],"/**\n *\n * Input\n *\n */\n\nimport React from \"react\";\nimport ReactStars from \"react-rating-stars-component\";\n\nconst Input = (props) => {\n  const {\n    type,\n    value,\n    error,\n    step,\n    decimals,\n    min,\n    max,\n    disabled,\n    placeholder,\n    rows,\n    label,\n    name,\n    onInputChange,\n    inlineElement,\n  } = props;\n\n  // const _onChange = (e) => {\n  //   if (e.target.name == \"image\") {\n  //     onInputChange(e.target.name, e.target.files[0]);\n  //   } else {\n  //     onInputChange(e.target.name, e.target.value);\n  //   }\n  // };\n\n  if (type === \"textarea\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <textarea\n          type={\"textarea\"}\n          // onChange={(e) => {\n          //   _onChange(e);\n          // }}\n          rows={rows}\n          name={name}\n          // value={value}\n          placeholder={placeholder}\n          className={\"textarea-text\"}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else if (type === \"number\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    const handleOnInput = (e) => {\n      if (!decimals) {\n        e.target.value = e.target.value.replace(/[^0-9]*/g, \"\");\n      }\n    };\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <input\n          autoComplete=\"on\"\n          step=\"step\"\n          min={min || 0}\n          max={max || null}\n          pattern=\"[0-9]\"\n          onInput={handleOnInput}\n          type={type}\n          // onChange={(e) => {\n          //   _onChange(e);\n          // }}\n          disabled={disabled}\n          name={name}\n          // value={value}\n          placeholder={placeholder}\n          className={\"input-number\"}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else if (type === \"stars\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <ReactStars\n          name={name}\n          starCount={5}\n          size={30}\n          color={\"#adb5bd\"}\n          activeColor={\"#ffb302\"}\n          a11y={true}\n          isHalf={false}\n          emptyIcon={<i className=\"fa fa-star\" />}\n          halfIcon={<i className=\"fa fa-star-half-alt\" />}\n          filledIcon={<i className=\"fa fa-star\" />}\n          value={value}\n          // onChange={(value) => {\n          //   onInputChange(name, value);\n          // }}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else {\n    const styles = `input-box${inlineElement ? ` inline-btn-box` : \"\"} ${\n      error ? \"invalid\" : \"\"\n    }`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <div className=\"input-text-block\">\n          <input\n            className={\"input-text\"}\n            autoComplete=\"on\"\n            type={type}\n            // onChange={(e) => {\n            //   _onChange(e);\n            // }}\n            disabled={disabled}\n            name={name}\n            // value={value}\n            placeholder={placeholder}\n          />\n          {inlineElement}\n        </div>\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  }\n};\n\nexport default Input;\n","/Users/apple/Downloads/MOON/client/src/components/cards/LoadingCard.js",[],"/Users/apple/Downloads/MOON/client/src/components/cards/ProductCard.js",["353","354","355"],"import React, { useState } from \"react\";\nimport { Card, Tooltip } from \"antd\";\nimport {\n  EyeOutlined,\n  ShoppingCartOutlined,\n  HeartOutlined,\n} from \"@ant-design/icons\"