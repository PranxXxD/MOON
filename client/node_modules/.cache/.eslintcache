t(res.data));\n  }, []);\n\n  const loadAllProducts = () => {\n    setLoading(true);\n    // sort, order, limit\n    getProducts(\"createdAt\", \"desc\", page).then((res) => {\n      setProducts(res.data);\n      setLoading(false);\n    });\n  };\n\n  return (\n    <>\n      <div className=\"container\">\n        {loading ? (\n          <LoadingCard count={3} />\n        ) : (\n          <div className=\"row\">\n            {products.map((product) => (\n              <div key={product._id} className=\"col-md-3\">\n                <ProductCard product={product} />\n              </div>\n            ))}\n          </div>\n        )}\n      </div>\n\n      <div className=\"row\">\n        <nav className=\"col-md-4 offset-md-4 text-center pt-5 p-3\">\n          <Pagination\n            current={page}\n            total={(productsCount / 3) * 10}\n            onChange={(value) => setPage(value)}\n          />\n        </nav>\n      </div>\n    </>\n  );\n};\n\nexport default NewArrivals;\n","/Users/apple/Downloads/MOON/client/src/components/category/CategoryList.js",[],"/Users/apple/Downloads/MOON/client/src/components/forms/Star.js",[],"/Users/apple/Downloads/MOON/client/src/components/forms/CategoryForm.js",[],"/Users/apple/Downloads/MOON/client/src/components/forms/FileUpload.js",[],"/Users/apple/Downloads/MOON/client/src/components/sub/SubList.js",[],["328","329"],"/Users/apple/Downloads/MOON/client/src/components/forms/ProductUpdateForm.js",["330","331"],"/Users/apple/Downloads/MOON/client/src/components/cards/AdminProductCard.js",[],"/Users/apple/Downloads/MOON/client/src/components/forms/ProductCreateForm.js",["332","333","334","335","336"],"/Users/apple/Downloads/MOON/client/src/components/forms/LocalSearch.js",[],"/Users/apple/Downloads/MOON/client/src/functions/user.js",[],"/Users/apple/Downloads/MOON/client/src/functions/rating.js",[],"/Users/apple/Downloads/MOON/client/src/components/modal/RatingModal.js",["337"],"import React, { useState } from \"react\";\ni