 <div onClick={handleModal}>\n        <Button\n          variant=\"primary\"\n          text={user ? \"Leave Rating\" : \"Login To Review\"}\n          icon={<ReviewIcon />}\n        />\n      </div>\n      <Modal\n        title=\"Leave Your Rating\"\n        centered\n        visible={modalVisible}\n        onOk={() => {\n          setModalVisible(false);\n          toast.success(\"Thanks for your review. It will appear soon\");\n        }}\n        onCancel={() => setModalVisible(false)}\n      >\n        {children}\n      </Modal>\n    </>\n  );\n};\n\nexport default RatingModal;\n","/Users/apple/Downloads/MOON/client/src/components/forms/Search.js",[],"/Users/apple/Downloads/MOON/client/src/components/Button/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/Icon/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/CartIcon/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/Carousel/CarouselContainer.js",[],"/Users/apple/Downloads/MOON/client/src/components/nav/Logo.js",[],"/Users/apple/Downloads/MOON/client/src/components/Input/index.js",["368","369"],"/**\n *\n * Input\n *\n */\n\nimport React from \"react\";\nimport ReactStars from \"react-rating-stars-component\";\n\nconst Input = (props) => {\n  const {\n    type,\n    value,\n    error,\n    step,\n    decimals,\n    min,\n    max,\n    disabled,\n    placeholder,\n    rows,\n    label,\n    name,\n    onInputChange,\n    inlineElement,\n  } = props;\n\n  // const _onChange = (e) => {\n  //   if (e.target.name == \"image\") {\n  //     onInputChange(e.target.name, e.target.files[0]);\n  //   } else {\n  //     onInputChange(e.target.name, e.target.value);\n  //   }\n  // };\n\n  if (type === \"textarea\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <textarea\n          type={\"textarea\"}\n          // onChange={(e) => {\n          //   _onChange(e);\n          // }}\n          rows={rows}\n          name={name}\n          // value={value}\n          placeholder={placeholder}\n          className={\"textarea-text\"}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else if (type === \"number\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    const handleOnInput = (e) => {\n      if (!decimals) {\n        e.target.value = e.target.value.replace(/[^0-9]*/g, \"\");\n      }\n    };\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <input\n          autoComplete=\"on\"\n          step=\"step\"\n          min={min || 0}\n          max={max || null}\n          pattern=\"[0-9]\"\n          onInput={handleOnInput}\n          type={type}\n          // onChange={(e) => {\n          //   _onChange(e);\n          // }}\n          disabled={disabled}\n          name={name}\n          // value={value}\n          placeholder={placeholder}\n          className={\"input-number\"}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else if (type === \"stars\") {\n    const styles = `input-box${error ? \" invalid\" : \"\"}`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <ReactStars\n          name={name}\n          starCount={5}\n          size={30}\n          color={\"#adb5bd\"}\n          activeColor={\"#ffb302\"}\n          a11y={true}\n          isHalf={false}\n          emptyIcon={<i className=\"fa fa-star\" />}\n          halfIcon={<i className=\"fa fa-star-half-alt\" />}\n          filledIcon={<i className=\"fa fa-star\" />}\n          value={value}\n          // onChange={(value) => {\n          //   onInputChange(name, value);\n          // }}\n        />\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  } else {\n    const styles = `input-box${inlineElement ? ` inline-btn-box` : \"\"} ${\n      error ? \"invalid\" : \"\"\n    }`;\n\n    return (\n      <div className={styles}>\n        {label && <label>{label}</label>}\n        <div className=\"input-text-block\">\n          <input\n            className={\"input-text\"}\n            autoComplete=\"on\"\n            type={type}\n            // onChange={(e) => {\n            //   _onChange(e);\n            // }}\n            disabled={disabled}\n            name={name}\n            // value={value}\n            placeholder={placeholder}\n          />\n          {inlineElement}\n        </div>\n        <span className=\"invalid-message\">{error && error[0]}</span>\n      </div>\n    );\n  }\n};\n\nexport default Input;\n","/Users/apple/Downloads/MOON/client/src/components/cards/LoadingCard.js",[],"/Users/apple/Downloads/MOON/client/src/components/cards/ProductCard.js",["370","371","372"],"import React, { useState } from \"react\";\nimport { Card, Tooltip } from \"antd\";\nimport {\n  EyeOutlined,\n  ShoppingCartOutlined,\n  HeartOutlined,\n} from \"@ant-design/icons\";\nimport laptop from \"../../images/laptop.jpg\";\nimport { Link } from \"react-router-dom\";\nimport { showAverage } from \"../../functions/rating\";\nimport _ from \"lodash\";\nimport { useDispatch } from \"react-redux\";\nimport \"./productcard.css\";\n\nconst { Meta } = Card;\n\nconst ProductCard = ({ product }) => {\n  const [tooltip, setTooltip] = useState(\"Click to add\");\n\n  //redux\n  const dispatch = useDispatch();\n\n  const handleAddToCart = () => {\n    //create array\n    let cart = [];\n    if (typeof window !== \"undefined\") {\n      //if cart is already in local storage\n      if (localStorage.getItem(\"cart\")) {\n        cart = JSON.parse(localStorage.getItem(\"cart\"));\n      }\n      //push new product to Cart\n      cart.push({\n        ...product,\n        count: 1,\n      });\n      //remove duplicates\n      let unique = _.uniqWith(cart, _.isEqual);\n      //save to local storage\n      console.log(\"unique\", unique);\n      localStorage.setItem(\"cart\", JSON.stringify(unique));\n\n      //show tooltip\n      setTooltip(\"Added\");\n\n      //add to redux state\n      dispatch({\n        type: \"ADD_TO_CART\",\n        payload: unique,\n      });\n\n      // show cart items in side drawer\n      dispatch({\n        type: \"SET_VISIBLE\",\n        payload: true,\n      });\n    }\n  };\n  // destructure\n  const { images, title, description, slug, price } = product;\n  return (\n    <>\n      {product && product.ratings && product.ratings.length > 0 ? (\n        showAverage(product)\n      ) : (\n        <div className=\"text-center pt-1 pb-3\"> No Ratings yet</div>\n      )}\n      <Card\n        style={{ cursor: \"pointer\" }}\n        cover={\n          <img\n            src={images && images.length ? images[0].url : laptop}\n            style={{ height: \"150px\", objectFit: \"cover\" }}\n            className=\"p-1\"\n          />\n        }\n        actions={[\n          <Link to={``}>\n            <HeartOutlined className=\"text-danger\" /> <br /> Add to Wishlist\n          </Link>,\n          <Tooltip title={tooltip}>\n            <a onClick={handleAddToCart}>\n              <ShoppingCartOutlined className=\"text-warning\" /> <br /> Add to\n              Cart\n            </a>\n          </Tooltip>,\n        ]}\n      >\n        <Link to={`product/${slug}`}>\n          <div>\n            <p className=\"price\">â‚¹{price}</p>\n          </div>\n          <Meta\n            title={\n              <span\n                className=\"ant-card-meta-title\"\n                style={{ color: \"#2962FF\" }}\n              >\n                {title}\n              </span>\n            }\n            description={`${description && description.substring(0, 40)}...`}\n          />\n        </Link>\n      </Card>\n    </>\n  );\n};\n\nexport default ProductCard;","/Users/apple/Downloads/MOON/client/src/components/cards