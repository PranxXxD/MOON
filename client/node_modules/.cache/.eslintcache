,{"filePath":"296","messages":"297","errorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":"298","usedDeprecatedRules":"149"},{"filePath":"299","messages":"300","errorCount":0,"warningCount":7,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"301","messages":"302","errorCount":0,"warningCount":11,"fixableErrorCount":0,"fixableWarningCount":0,"source":"303","usedDeprecatedRules":"149"},{"filePath":"304","messages":"305","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"149"},{"filePath":"306","messages":"307","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"308","usedDeprecatedRules":"149"},"/Users/apple/Downloads/MOON/client/src/index.js",[],["309","310"],"/Users/apple/Downloads/MOON/client/src/App.js",[],"/Users/apple/Downloads/MOON/client/src/reportWebVitals.js",[],"/Users/apple/Downloads/MOON/client/src/reducers/index.js",[],"/Users/apple/Downloads/MOON/client/src/components/nav/Header.js",["311"],"import React, { useState } from \"react\";\nimport Button from \"../Button\";\nimport { Link, NavLink as ActiveLink } from \"react-router-dom\";\nimport {\n  Container,\n  Row,\n  Col,\n  Navbar,\n  Nav,\n  NavItem,\n  NavLink,\n  UncontrolledDropdown,\n  DropdownToggle,\n  DropdownMenu,\n  DropdownItem,\n} from \"reactstrap\";\nimport firebase from \"firebase\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport CartIcon from \"../CartIcon\";\nimport { BarsIcon } from \"../Icon\";\nimport Search from \"../forms/Search\";\nimport Logo from \"./Logo\";\n\nconst Header = () => {\n  const [current, setCurrent] = useState(\"home\");\n\n  const dispatch = useDispatch();\n  const { user, cart } = useSelector((state) => ({ ...state }));\n  let history = useHistory();\n\n  const handleClick = (e) => {\n    setCurrent(e.key);\n  };\n\n  const logout = () => {\n    firebase.auth().signOut();\n    dispatch({\n      type: \"LOGOUT\",\n      payload: null,\n    });\n    history.push(\"/login\");\n  };\n\n  const handleAddToCart = () => {\n    // show cart items in side drawer\n    dispatch({\n      type: \"SET_VISIBLE\",\n      payload: true,\n    });\n  };\n\n  return (\n    <header className=\"header fixed-mobile-header\">\n      <div className=\"header-info\">\n        <Container onClick={handleClick} selectedKeys={[current]}>\n          <Row>\n            <Col md=\"4\" className=\"text-center d-none d-md-block\">\n              <i className=\"fa fa-truck\" />\n              <span>Free Shipping</span>\n            </Col>\n            <Col md=\"4\" className=\"text-center d-none d-md-block\">\n              <i className=\"fa fa-credit-card\" />\n              <span>Payment Methods</span>\n            </Col>\n            <Col md=\"4\" className=\"text-center d-none d-md-block\">\n              <i className=\"fa fa-phone\" />\n              <span>Call Us: 79777-89547</span>\n            </Col>\n            <Col xs=\"12\" className=\"text-center d-block d-md-none\">\n              <i className=\"fa fa-phone\" />\n              <span> Need advice? Call us 951-999-9999</span>\n            </Col>\n          </Row>\n        </Container>\n      </div>\n      <Container>\n        <Row className=\"top-header\">\n          <Col\n            xs={{ size: 12, order: 1 }}\n            sm={{ size: 12, order: 1 }}\n            md={{ size: 3, order: 1 }}\n            lg={{ size: 3, order: 1 }}\n          >\n            <div className=\"brand\">\n              <Link to=\"/\" className=\"logo\">\n                <Logo />\n              </Link>\n            </div>\n          </Col>\n          <Col\n            xs={{ size: 12, order: 2 }}\n            sm={{ size: 12, order: 2 }}\n            md={{ size: 4, order: 1 }}\n            lg={{ size: 5, order: 3 }}\n            className=\"desktop-hidden\"\n          >\n            <div className=\"header-links\">\n              <Button ariaLabel=\"open the menu\" icon={<BarsIcon />} />\n              <Link to=\"/\" className=\"logo\">\n                <Logo />\n              </Link>\n              <Search />\n              <CartIcon cartItems={cart.length} onClick={handleAddToCart} />\n            </div>\n          </Col>\n          <Col\n            xs={{ size: 12, order: 2 }}\n            sm={{ size: 12, order: 2 }}\n            md={{ size: 9, order: 1 }}\n            lg={{ size: 9, order: 3 }}\n          >\n            <Navbar color=\"light\" light expand=\"md\" className=\"mt-1 mt-md-0\">\n              <CartIcon\n                className=\"d-none d-md-block\"\n                cartItems={cart.length}\n                onClick={handleAddToCart}\n              />\n              <span className=\"search float-right p-1 ml-1 mr-1\">\n                <Search />\n              </span>\n              <Nav navbar>\n                <NavItem>\n                  <NavLink tag={ActiveLink} to=\"/shop\" activeClassName=\"active\">\n                    Shop\n                  </NavLink>\n                </NavItem>\n                {user ? (\n                  <UncontrolledDropdown nav inNavbar>\n                    <DropdownToggle nav>\n                      {user ? user.email.split(\"@\")[0] : \"Welcome\"}\n                      <span className=\"fa fa-chevron-down dropdown-caret\"></span>\n                    </DropdownToggle>\n                    {user.role == \"admin\" ? (\n                      <DropdownMenu right>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/history\")}\n                        >\n                          Dashboard\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/product\")}\n                        >\n                          Product\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/products\")}\n                        >\n                          Products\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/category\")}\n                        >\n                          category\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/sub\")}\n                        >\n                          sub\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/admin/coupon\")}\n                        >\n                          coupon\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/password\")}\n                        >\n                          Settings\n                        </DropdownItem>\n                        <DropdownItem onClick={logout}>Sign Out</DropdownItem>\n                      </DropdownMenu>\n                    ) : (\n                      <DropdownMenu right>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/history\")}\n                        >\n                          Dashboard\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/wishlist\")}\n                        >\n                          Wishlist\n                        </DropdownItem>\n                        <DropdownItem\n                          onClick={() => history.push(\"/user/password\")}\n                        >\n                          Settings\n                        </DropdownItem>\n                        <DropdownItem onClick={logout}>Sign Out</DropdownItem>\n                      </DropdownMenu>\n                    )}\n                  </UncontrolledDropdown>\n                ) : (\n                  <UncontrolledDropdown nav inNavbar>\n                    <DropdownToggle nav>\n                      Welcome!\n                      <span className=\"fa fa-chevron-down dropdown-caret\"></span>\n                    </DropdownToggle>\n                    <DropdownMenu right>\n                      <DropdownItem onClick={() => history.push(\"/login\")}>\n                        Login\n                      </DropdownItem>\n                      <DropdownItem onClick={() => history.push(\"/register\")}>\n                        Sign Up\n                      </DropdownItem>\n                    </DropdownMenu>\n                  </UncontrolledDropdown>\n                )}\n              </Nav>\n            </Navbar>\n          </Col>\n        </Row>\n      </Container>\n\n      {/* hidden cart drawer */}\n      {/* <div\n        className={isCartOpen ? \"mini-cart-open\" : \"hidden-mini-cart\"}\n        aria-hidden={`${isCartOpen ? false : true}`}\n      >\n        <div className=\"mini-cart\">\n          <SideDrawer />\n          Cart\n        </div>\n        <div\n          className={\n            isCartOpen ? \"drawer-backdrop dark-overflow\" : \"drawer-backdrop\"\n          }\n          onClick={toggleCart}\n        />\n      </div> */}\n\n      {/* //hidden menu drawer\n        <div\n          className={isMenuOpen ? 'mini-menu-open' : 'hidden-mini-menu'}\n          aria-hidden={`${isMenuOpen ? false : true}`}\n  \n            Menu\n          </div>\n          <div\n            className={\n              isMenuOpen ? 'drawer-backdrop dark-overflow' : 'drawer-backdrop'\n            }\n            onClick={toggleMenu}\n          />\n        </div>  */}\n    </header>\n  );\n};\n\nexport default Header;\n","/Users/apple/Downloads/MOON/client/src/firebase.js",[],"/Users/apple/Downloads/MOON/client/src/pages/Home.js",[],"/Users/apple/Download