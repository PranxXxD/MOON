    <Button\n                      variant=\"primary\"\n                      disabled={quantity == 0}\n                      text=\"Remove From Bag\"\n                      className=\"bag-btn\"\n                      icon={<BagIcon />}\n                      onClick={() => handleRemove(_id)}\n                    />\n                  ) : (\n                    <Button\n                      variant=\"primary\"\n                      disabled={quantity == 0}\n                      text=\"Add To Bag\"\n                      className=\"bag-btn\"\n                      icon={<BagIcon />}\n                      onClick={handleAddToCart}\n                    />\n                  )}\n                  <div className=\"mt-5\">\n                    <RatingModal>\n                      <StarRating\n                        name={_id}\n                        numberOfStars={5}\n                        rating={star}\n                        changeRating={onStarClick}\n                        isSelectable={true}\n                        starRatedColor=\"red\"\n                      />\n                    </RatingModal>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </Col>\n        </Row>\n        <Tabs type=\"card\">\n          <TabPane tab=\"More\" key=\"1\">\n            Call us at +91 7977789547 if you have any Queries regarding this\n            product\n          </TabPane>\n          <TabPane tab=\"Description\" key=\"2\">\n            {description && description}\n          </TabPane>\n        </Tabs>\n      </>\n    </div>\n  );\n};\n\nexport default SingleProduct;\n","/Users/apple/Downloads/MOON/client/src/functions/contact.js",[],"/Users/apple/Downloads/MOON/client/src/pages/Contact.js",["384","385"],"import React, { useState, useEffect } from \"react\";\nimport { toast } from \"react-toastify\";\nimport { Row, Col } from \"reactstrap\";\nimport Button from \"../components/Button\";\nimport { createContact } from \"../functions/contact\";\n\nconst Contact = () => {\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [phone, setPhone] = useState(\"\");\n  const [message, setMessage] = useState(\"\");\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    // console.table(name, email, phone, message);\n    createContact({ name, email, phone, message })\n      .then((res) => {\n        setName(\"\");\n        setEmail(\"\");\n        setPhone(\"\");\n        setMessage(\"\");\n        toast.success(\n          `${res.data.name} your message is received. we will contact you shortly.`\n        );\n      })\n      .catch((err) => console.log(\"contact req error\", err));\n  };\n\n  return (\n    <div className=\"contact\">\n      <h2>Contact Information</h2>\n      <hr />\n      <form>\n        <Row>\n          <Col xs=\"6\" md=\"3\">\n            <label>Name</label>\n            <input\n              type={\"text\"}\n              name={\"name\"}\n              placeholder={\"Your Full Name\"}\n              value={name}\n              required\n              autoFocus\n              onChange={(e) => setName(e.target.value)}\n            />\n          </Col>\n          <Col xs=\"6\" md=\"3\">\n            <label>Phone</label>\n            <input\n              required\n              type={\"text\"}\n              name={\"phone\"}\n              placeholder={\"Your Phone Number\"}\n              value={phone}\n              onChange={(e) => setPhone(e.target.value)}\n            />\n          </Col>\n          <Col xs=\"12\" md=\"6\">\n            <label>Email</label>\n            <input\n              required\n              type={\"text\"}\n              required\n              name={\"email\"}\n              placeholder={\"Your Email Address\"}\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n            />\n          </Col>\n          <Col xs=\"12\" md=\"12\" className=\"mt-5\">\